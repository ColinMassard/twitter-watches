{
  "name": "boilerplate-prismic",
  "version": "1.0.0",
  "description": "Basic boilerplate for full Javascript project with Prismic",
  "scripts": {
    "backend:build": "node app.js",
    "backend:development": "nodemon app.js",
    "frontend:build": "webpack --progress --config webpack.config.build.js",
    "frontend:development": "webpack serve --progress --config webpack.config.development.js",
    "start": "concurrently --kill-others \"npm run backend:development\" \"npm run frontend:development\""
  },
  "author": "Colin Massard",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.2.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "errorhandler": "^1.5.1",
    "eslint": "^8.42.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-webpack-plugin": "^4.0.1",
    "file-loader": "^6.2.0",
    "glslify-loader": "^2.0.0",
    "gsap": "^3.12.1",
    "image-minimizer-webpack-plugin": "^3.8.2",
    "include-media": "^2.0.0",
    "mini-css-extract-plugin": "^2.7.6",
    "normalize-wheel": "^1.0.1",
    "ogl": "^0.0.117",
    "postcss-loader": "^7.3.3",
    "prefix": "^1.0.0",
    "raw-loader": "^4.0.2",
    "sass": "^1.63.3",
    "sass-loader": "^13.3.2",
    "tailwindcss": "^3.3.2",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "dependencies": {
    "@prismicio/client": "5.1.0",
    "body-parser": "^1.20.2",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.22",
    "postcss-preset-env": "^8.5.1",
    "prismic-dom": "^2.2.7",
    "pug": "^3.0.2",
    "ua-parser-js": "^1.0.35"
  }
}
